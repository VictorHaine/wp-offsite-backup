JOB_NAME="WordPress partial backup (database only)"
BACKUP_NAME=wordpress-backup-`date +%Y-%m-%d_%H-%M-%S`
COMPRESS=gzip
MAX_SAVED_FILES=12

WP_ROOT_PATH=/var/www/html
DB_NAME=wordpress
MYSQL_EXTRA_FILE=.my.cnf
MYSQL_FILE=wordpress.sql

S3_URI=
AWS_PROFILE=

EXCLUDE_EXTRA=(
  ".git"
  ".DS_Store"
)

EXCLUDE_WP_CONTENT=(
  "./wp-content"
)

EXCLUDE_WP_CORE=(
  "./*.txt"
  "./*.html"
  "./*.php"
  "./wp-admin"
  "./wp-includes"
)

LOG_FILE=history.log
LOG_FORMAT=common
MAX_LOG_LINES=300
